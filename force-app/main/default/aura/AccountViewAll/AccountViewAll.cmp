<aura:component
    implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,Lightning:actionOverride,lightning:isUrlAddressable" access="global" >
    <!--필요한 인터페이스 호출하기 -->

    <!--아이디 값 가져오기-->
    <aura:attribute name="recordId" type="String" />

    <!--c:클래스 이름 넣기-->
    <div>
        <c:testViewAll recordId="{!v.recordId}"></c:testViewAll>
    </div>
</aura:component>